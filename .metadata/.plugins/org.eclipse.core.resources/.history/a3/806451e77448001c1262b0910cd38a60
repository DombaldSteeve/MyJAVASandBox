package Immeuble;

public class Appartement {
	private String numero;
	
	public Appartement(String numero) {
		super();
		this.numero = numero;
	}
	
	public String getNumero() {
		return numero;
	}
	
	public void setNumero(String numero) {
		this.numero = numero;
	}
	
	@Override
	public String toString() {
		return "(Apt " + numero + ")";
	}
}
